{
  "name": "jspm-react-es6-starter",
  "version": "1.0.0",
  "description": "A starter kit for react with JSPM in ES6",
  "main": "app/main.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register tests/*.test.js",
    "lint": "./node_modules/.bin/eslint ./app --ext .js --ignore-pattern internals/scripts || true",
    "build": "jspm bundle app/main --inject --minify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sahiljain112/jspm-react-es6-starter.git"
  },
  "keywords": [
    "es6",
    "react",
    "jspm",
    "starter",
    "development"
  ],
  "author": "sahiljain112@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "glob": "^7.0.6",
    "jspm": "^0.16.45",
    "mocha": "^3.0.2",
    "systemjs": "^0.19.37"
  },
  "jspm": {
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.27",
      "react": "npm:react@^15.3.1",
      "react-bootstrap": "npm:react-bootstrap@^0.30.3",
      "react-dom": "npm:react-dom@^15.3.1"
    },
    "devDependencies": {
      "babel-register": "npm:babel-register@^6.14.0",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
